<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".usernakonlogina">

    <TextView
        android:id="@+id/tvnakonlogina1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="125dp"
        android:text="TextView"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="163dp" />

    <TextView
        android:id="@+id/tvnakonlogina2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/tvnakonlogina1"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="-88dp"
        android:text="TextView"
        app:layout_constraintTop_toBottomOf="@+id/tvnakonlogina1"
        tools:layout_editor_absoluteX="163dp" />

    <TextView
        android:id="@+id/tvNakonLogina3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="204dp"
        android:text="TextView"
        app:layout_constraintTop_toBottomOf="@+id/tvnakonlogina2"
        tools:layout_editor_absoluteX="163dp" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/actionButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="29dp"
        android:layout_marginRight="38dp"
        android:clickable="true"
        app:srcCompat="@drawable/sparkairlines"
        tools:layout_editor_absoluteX="304dp"
        tools:layout_editor_absoluteY="426dp" />
</RelativeLayout>